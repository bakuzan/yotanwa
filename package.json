{
  "name": "yotanwa",
  "version": "3.0.0",
  "private": true,
  "description": "Display your anime & manga lists as ranked tiers",
  "main": "index.jsx",
  "scripts": {
    "dev": "nodemon",
    "build": "next build && tsc --project tsconfig.server.json",
    "start": "NODE_ENV=production node dist/server/index.js",
    "lint": "tslint --fix -c tslint.json \"*/**/*.ts\""
  },
  "author": "bakuzan",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bakuzan/yotanwa.git"
  },
  "license": "ISC",
  "dependencies": {
    "@next/bundle-analyzer": "^9.5.3",
    "@zeit/next-sass": "1.0.1",
    "autoprefixer": "10.0.1",
    "ayaka": "npm:bkz-ayaka@^2.1.0",
    "classnames": "^2.2.6",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mal-scraper": "^2.11.3",
    "meiko": "npm:bkz-meiko@^6.1.3",
    "mongoose": "^5.10.7",
    "nano-css": "^5.3.0",
    "next": "^9.5.3",
    "next-offline": "^5.0.2",
    "node-fetch": "^2.6.1",
    "node-sass": "^4.14.1",
    "postcss-scss": "^3.0.2",
    "react": "^16.13.1",
    "react-beautiful-dnd": "^11.0.5",
    "react-dom": "^16.13.1"
  },
  "lint-staged": {
    "*.{json}": [
      "prettier --single-quote false --write"
    ],
    "*.{js,ts,tsx}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "yarn run lint"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/plugin-proposal-optional-chaining": "^7.11.0",
    "@types/express": "^4.17.1",
    "@types/mongoose": "^5.7.6",
    "@types/node": "^13.9.1",
    "@types/node-fetch": "^2.5.5",
    "@types/react-beautiful-dnd": "^12.1.2",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.0",
    "nodemon": "^1.19.2",
    "prettier": "^2.1.2",
    "prop-types": "^15.7.2",
    "ts-node": "^9.0.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "^3.8.3"
  },
  "browserslist": [
    ">1%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}

{
  "name": "yotanwa",
  "version": "3.0.0",
  "private": true,
  "description": "Display your anime & manga lists as ranked tiers",
  "main": "index.jsx",
  "scripts": {
    "dev": "nodemon",
    "build": "next build && tsc --project tsconfig.server.json",
    "start": "NODE_ENV=production node dist/server/index.js",
    "lint": "tslint --fix -c tslint.json \"*/**/*.ts\"",
    "prepare": "husky install"
  },
  "author": "bakuzan",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bakuzan/yotanwa.git"
  },
  "license": "ISC",
  "dependencies": {
    "@next/bundle-analyzer": "^12.0.10",
    "autoprefixer": "10.4.2",
    "ayaka": "npm:bkz-ayaka@^2.1.0",
    "classnames": "^2.3.1",
    "dotenv": "^10.0.0",
    "express": "^4.20.0",
    "mal-scraper": "^2.11.4",
    "meiko": "npm:bkz-meiko@^6.2.1",
    "mongoose": "^8.8.3",
    "nano-css": "^5.3.4",
    "next": "^14.2.10",
    "next-offline": "^5.0.5",
    "node-fetch": "^3.2.0",
    "postcss-scss": "^4.0.3",
    "react": "^17.0.2",
    "react-beautiful-dnd": "^13.1.0",
    "react-dom": "^17.0.2",
    "sass": "^1.49.7"
  },
  "lint-staged": {
    "*.{json}": [
      "prettier --single-quote false --write"
    ],
    "*.{js,ts,tsx}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "yarn run lint"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.16.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.16.7",
    "@babel/plugin-proposal-optional-chaining": "^7.16.7",
    "@types/express": "^4.17.13",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^16.11.22",
    "@types/node-fetch": "^3.0.3",
    "@types/react-beautiful-dnd": "^13.1.2",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "husky": "^7.0.4",
    "lint-staged": "^11.2.6",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "prop-types": "^15.8.1",
    "ts-node": "^10.4.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "^4.5.5"
  },
  "browserslist": [
    ">1%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
